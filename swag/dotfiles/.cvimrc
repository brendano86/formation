" GitLab

map <Leader>glt  :tabopen https://about.gitlab.com/team/<CR>
map <Leader>glh  :tabopen https://about.gitlab.com/handbook/<CR>

site '*://gitlab.*' {
  unmap a 
  unmap e 
  unmap s
  unmap r 
  unmap i 
  unmap g 
  unmap l 
  unmap m 
  unmap b 
  unmap p 
  unmap g?
}

site '*://*.reddit.com/*' {
  unmap j k x
  set numerichints
}

site '*://news.ycombinator.com/*' {
  unmapAll
}

site '*://twitter.com/*' {
  unmap j k 
}

site '*://8bbit.com/*' {
   unmapAll
}

site '*://*.facebook.com/*' {
  unmap j k 
}
site "https://mail.google.com/*" {
    unmapAll
    map <C-l> nextTab
    map <C-h> previousTab
    map : openCommandBar
}

site "*://calendar.google.com/*" {
    call :pintab
}

site "*://trello.com/*" {
    unmapAll
    map <C-l> nextTab
    map <C-h> previousTab
    map : openCommandBar
}

site "*://localhost*" {
   unmapAll
}

site "://olearycrew.atlassian.net/*" {
  unmapAll
}

site "://gitlab-training.gitlab.io/*" {
  unmapAll
}

site "*://gitlab.com/*" {
  unmap ? i g t 
}

map <C-l> nextTab
map <C-h> previousTab

